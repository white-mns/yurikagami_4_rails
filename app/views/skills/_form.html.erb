  <div class="field">
    <%= form.label :e_no %>
    <%= form.number_field :e_no, id: :skill_e_no %>
  </div>

  <div class="field">
    <%= form.label :sub_no %>
    <%= form.number_field :sub_no, id: :skill_sub_no %>
  </div>

  <div class="field">
    <%= form.label :skill_no %>
    <%= form.number_field :skill_no, id: :skill_skill_no %>
  </div>

  <div class="field">
    <%= form.label :skill_id %>
    <%= form.number_field :skill_id, id: :skill_skill_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
